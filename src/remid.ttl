http://github.com/ssj71/reMID.lv2>
        a lv2:Plugin, lv2:InstrumentPlugin ;
        doap:name "reMID" ;
        doap:license <http://opensource.org/licenses/GPL-2.0> ;
        doap:description "Emulation of a SID chip";
        doap:maintainer [
                foaf:name "Spencer Jackson" ;
                foaf:homepage <http://ssj71.github.io/infamousPlugins> ;
                foaf:mbox <mailto:ssjackson71@gmail.com> ;
        ] ;
        lv2:minorVersion 0 ;
        lv2:microVersion 1 ;
        rdfs:comment "This midi controlled SID emulator loads instrument files that control the chip for internal, per-note automation. It also allows for chiptune drumkits to be loaded." ;
        lv2:requiredFeature <http://lv2plug.in/ns/ext/urid#map> ;
        lv2:optionalFeature lv2:hardRTCapable ;

        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "OUTPUT_R" ;
                lv2:name "Right Out" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "OUTPUT_L" ;
                lv2:name "Left Out" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2atom:AtomPort ;
                lv2atom:bufferType lv2atom:Sequence ;
                lv2atom:supports lv2midi:MidiEvent ;
#                lv2atom:supports lv2time:Position ;
                lv2:index 2 ;
                lv2:symbol "MIDI_IN";
                lv2:name "MIDI In";
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2atom:AtomPort ;
                lv2atom:bufferType lv2atom:Sequence ;
                lv2:index 3 ;
                lv2:symbol "NOTIFY";
                lv2:name "Notify";
        ].

<http://ssj71.github.io/infamousPlugins/plugs.html#casynth:preset:00Dfault>
        a pset:Preset ;
        lv2:appliesTo <http://ssj71.github.io/infamousPlugins/plugs.html#casynth> ;
        rdfs:label "00 Dfault" ;
        rdfs:seeAlso <casynth_presets.ttl> .
